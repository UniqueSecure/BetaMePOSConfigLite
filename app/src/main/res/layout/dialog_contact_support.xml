<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/imageDialogLayout"
    android:layout_width="@dimen/dialog_with"
    android:layout_height="@dimen/dialog_height250"
    android:gravity="center_vertical|center_horizontal"
    android:orientation="vertical">
    
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="150dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/dialog_image_contact_support" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/space10" />

    <TextView
        android:id="@+id/textContactSupport"
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dialog_ccontact_support"
        android:textSize="@dimen/text_size20" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/space10" />

    <Button
        android:id="@+id/btngo"
        android:layout_width="@dimen/button_width100"
        android:layout_height="wrap_content"
        android:background="@drawable/button_border"
        android:backgroundTint="@android:color/white"
        android:text="@string/next"
        android:textAllCaps="true"
        android:textColor="@color/colorCosmeticBlue"
        android:textSize="@dimen/text_size20"
        android:visibility="visible" />
</LinearLayout>